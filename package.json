{
  "name": "@future-house/feathers",
  "version": "0.3.0",
  "private": true,
  "description": "Future House Design System - A React component library based on shadcn/ui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./lib/styles/index.css": "./dist/lib/styles/index.css",
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./dist/lib/*.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "default": "./dist/icons/index.js"
    }
  },
  "keywords": [
    "react",
    "component-library",
    "design-system",
    "shadcn"
  ],
  "author": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-storybook": "storybook build",
    "build": "node scripts/build.js",
    "clean": "rm -rf dist",
    "deploy-storybook": "npm run build-storybook && touch ./storybook-static/.nojekyll && gh-pages -d ./storybook-static -t true",
    "dev": "npm run build -- --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "gen:icons": "node scripts/generate-icons.js",
    "lint": "eslint . --ext .ts,.tsx",
    "prebuild": "npm run format:check && npm run lint && npm run clean && npm run typecheck  && npm run gen:icons",
    "storybook": "storybook dev -p 6006",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.523.0",
    "react-day-picker": "^9.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/addon-links": "^9.0.13",
    "@storybook/addon-themes": "^9.0.13",
    "@storybook/react": "^9.0.13",
    "@storybook/react-vite": "^9.0.13",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "cpx2": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^9.0.13",
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^9.0.13",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  }
}
